classHuman{
//Overridden method
publicvoid eat()
{
System.out.println("Human is eating");
}
}
classBoyextendsHuman{
//Overriding method
publicvoid eat(){
System.out.println("Boy is eating");
}
publicstaticvoid main(String args[]){
Boy obj =newBoy();
//This will call the child class version of eat()
      obj.eat();
}
}
